<!DOCTYPE html>
<html>
<head>
  <title>Gcamp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav class="navbar navbar-default navbar-inverse navbar-fixed-top">
    <div class="container container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <%= link_to "gCamp", root_path, class: "navbar-brand" %>
      </div>
    </div>
  </nav>


  <div class="container">

    <div id="notice" class="alert alert-success" role="alert">
      <% if notice %>
        <%= notice %>
      <% end %>
    </div>



    <%= yield %>

    <footer class="footer">
      <div class="left_justify">
        <%= link_to "About", about_path %> |
        <%= link_to "Terms", terms_path %> |
        <%= link_to "Tasks", tasks_path %> |
        <%= link_to "FAQ", faq_path %>
      </div>
      <div class="right_justify">
        <div class="badges">
          <%= link_to "http://www.linkedin.com/in/kathrynkilianvovk/" do %>
            <%= image_tag("linked_in.jpg", height:'30') %>
          <% end %>
        </div>
        <div class="badges">
          <%= link_to "https://www.github.com/katekilianvovk" do %>
            <%= image_tag("GitHub.png", height:'30') %>
          <% end %>
        </div>
        <div class="badges">
          <%= link_to "https://twitter.com/katekilianvovk" do %>
            <%= image_tag("Twitter.png", height:'30') %>
          <% end %>
        </div>
        <div class="badges">
          <%= link_to "https://students.gschool.it/students/285" do %>
            <%= image_tag("galvanize.png", height:'30') %>
          <% end %>
        </div>
      </div>
    </footer>


  </div><!--container-->

</body>
</html>
