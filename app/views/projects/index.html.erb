<div class="page-header">
  <div class="pull-right">
      <%= link_to "New Project", new_project_path, class: 'btn btn-info' %>
  </div>
  <h1>Projects</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>
        Name
      </th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project|  %>
    <tr>
      <td>
        <%= project.name %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
