<div class="page-header">
  <h1>Listing Tasks</h1>
</div>

<h3>Description</h3>

<% @tasks.each do |task| %>
  <%= task.description %>
  <%= link_to "Show", task_path(task) %>
  <%= link_to "Edit", edit_task_path(task) %>
  <%= link_to "Destroy", task_path(task), method: :delete %>
  <br/>
<% end %>
<br/>
<%= link_to "New Task", new_task_path %>
